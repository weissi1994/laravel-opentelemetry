{
  "name": "seanhood/laravel-opentelemetry",
  "description": "Laravel middleware to send events to OpenTelemetry",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SeanHood\\LaravelOpenTelemetry\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SeanHood\\LaravelOpenTelemetry\\LaravelOpenTelemetryServiceProvider"
      ]
    }
  },
  "repositories": [
    { "type": "github", "url": "https://github.com/weissi1994/opentelemetry-php.git" }
  ],
  "scripts": {
    "format": "phpcbf --standard=psr2 src/"
  },
  "require-dev": {
    "orchestra/testbench": "v4.18.0",
    "squizlabs/php_codesniffer": "^3.5",
    "open-telemetry/opentelemetry": "dev-main",
    "laravel/framework": "v6.20.44",
    "phpunit/phpunit": "9.5.26",
    "guzzlehttp/psr7": "2.4.3",
    "guzzlehttp/guzzle": "7.5.0",
    "php-http/guzzle7-adapter": "1.0.0"
  }
}
